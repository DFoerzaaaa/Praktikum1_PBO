public class Soal2 {

    public static void main(String[] args) {
        int i = 42; // Mendeklarasikan variabel integer dengan nilai 42
        String s = (i < 40) ? "life" : (i > 50) ? "universe" : "everything";
        // Menggunakan operator ternary untuk menentukan nilai variabel 's'

        System.out.println(s); // Mencetak nilai dari variabel 's'
    }
}
